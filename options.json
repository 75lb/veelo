{
	"options": {
		"default": {
			"handbrake": {
				"preset": "Normal",
				"optimize": true,
				"srt-codeset": "UTF-8", 
				"srt-lang": "eng"
			},
			"handbraker": {
				"ext": "m4v",
				"preserve-dates": true
			}
		}
	},
	"ignoreList": [".DS_Store"],
	"archiveDirectory": "handbraker-originals"
}